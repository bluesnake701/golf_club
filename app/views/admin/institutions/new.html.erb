<h1>新施設登録</h1>
<%= form_with model: @institution, url: admin_institutions_path do |f| %>
<table>
  <tr>
    <td><%= f.file_field :image, accept: "image/*" %></td>
  </tr>
  <tr>
　  <td><%= f.label :name, "施設名" %></td>
　  <td><%= f.text_field :name %></td>
  </tr>
  <tr>
　  <td><%= f.label :introduction, "施設説明文" %></td>
　  <td><%= f.text_area :introduction %></td>
  </tr>
  <tr>
　  <td><%= f.submit '新規登録' %></td>
　</tr>
</table>
<% end %>

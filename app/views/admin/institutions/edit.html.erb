<h1>施設編集</h1>
<%= form_with model: @institution, url: admin_institution_path(@institution.id) do |f| %>
<table>
  <tr>
    <td><%= f.file_field :image, accept: "image/*" %></td>
  </tr>
  <tr>
　  <td><%= f.label :name, "施設名" %></td>
　  <td><%= f.text_field :name %></td>
  </tr>
  <tr>
　  <td><%= f.label :introduction, "施設説明文" %></td>
　  <td><%= f.text_area :introduction %></td>
  </tr>
  <tr>
　  <td><%= f.submit '編集する' %></td>
　</tr>
</table>
<% end %>